###########################################################################
#                            TextMate Snippets                            #
###########################################################################

#Elements------------------------------------
snippet get "Get Elements" i
getElement${1/(T)|.*/(?1:s)/}By${1:T}${1/(T)|(I)|.*/(?1:agName)(?2:d)/}('$2')$0
endsnippet

snippet oo "{}" i
{$1}$0
endsnippet

snippet ll "()" i
($1)$0
endsnippet

snippet : "Object key key: 'value'" i
${1:'${2:key}'}: ${3:'${4:value}'}$0
endsnippet

snippet doc "document" i
document.$0
endsnippet

snippet crele "createElement" i
createElement('$1')$0
endsnippet

#Function------------------------------------
snippet fun "function" !b
//$1{{{
const ${1:name} = function($2) {
	$3
};$0
//}}}
endsnippet

snippet func "function" i
function($1) {$2}$0
endsnippet

snippet -- "function" i
$1 => $2
endsnippet

snippet == "function" i
($1) => {
	$2
}$0
endsnippet

snippet >> "function" i
//$1{{{
${1:name} = ($2) => {
	$3
}$0
//}}}
endsnippet

#Language------------------------------------
snippet class "javascript class" !b
//$1{{{
class ${1:class_name} {
	constructor(${2:param}) {
		$3
	}
}$0
//}}}
endsnippet

snippet eclass "javascript class" !b
//$1{{{
class ${1:class_name} extends ${2:parent_class} {
	constructor(${3:param}) {
		super(${4:param});
		$5
	}
}$0
//}}}
endsnippet

snippet let "javascript let" !b
let $1 = $2;$0
endsnippet

snippet const "javascript const" !b
const $1 = $2;$0
endsnippet

snippet for "javascript for" !b
for (let i = 0; i < $1; i++) {
	$0
}
endsnippet

snippet forin "javascript for" !b
for (let ${1:a} in ${2:b}) {
	$0
}
endsnippet

snippet forof "javascript for" !b
for (let ${1:a} of ${2:b}) {
	$0
}
endsnippet

snippet "\bif" "javascript if" r
if ($1) {
	$0
}
endsnippet

snippet ife "javascript else" !b
if ($1) {
	$0
} else {
}
endsnippet

snippet "\belse" "javascript else" r
else {
	$0
}
endsnippet

snippet sw "javascript" !b
switch($1)
{
	case '$2':
		$3
		break;
	default:
		$0
}
endsnippet

snippet case "javascript" !b
case '$1':
	$2
	break;
$0
endsnippet

snippet while "javascript" !b
while ($1) {
	$0
}
endsnippet

