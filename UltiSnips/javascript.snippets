#console-----------------------------------
snippet log "console.log" i
console.log($1);$0
endsnippet

snippet con "assert 判断表达式是否为真"
console.assert($1);$0
endsnippet

snippet dir "dir"
console.dir($1);$0
endsnippet

snippet con "info"
console.info("$1"${2:,$3});$0
endsnippet

snippet con "time"
console.time('----- Timer $1-----');
console.timeEnd('----- Timer $2-----');
endsnippet

snippet con "trace 跟踪"
console.trace();
endsnippet

snippet con "profile 性能分析"
console.profile('--- Start profile');
console.profileEnd();
endsnippet

extends jquery, react

